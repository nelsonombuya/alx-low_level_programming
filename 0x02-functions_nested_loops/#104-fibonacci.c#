#include <stdio.h>

/**
 * main - Entry Point
 * Description - Prints the first 98 Fibonacci numbers, starting with 1
 * Return: Always 0.
 */
int main() {
    int i;
    unsigned long int a = 1, b = 2, next;

    printf("%u, %u, ", a, b);

    for(i = 2; i < 98; i++) {
        next = a + b;
        printf("%u", next);
        if(i != 97) {
            printf(", ");
        }
        a = b;
        b = next;
    }

    printf("\n");
    return 0;
}